<!DOCTYPE html>
<html lang="en">
<head>

    <!-- Basic Page Needs
    ================================================== -->
    <meta charset="utf-8">
    <title>Constra - Construction Html5 Template</title>

    <!-- Mobile Specific Metas
    ================================================== -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Construction Html5 Template">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=5.0">

    <!-- Favicon
    ================================================== -->
    <link rel="icon" type="image/png" href="~/clientside/images/favicon.png">

    <!-- CSS
    ================================================== -->
    <!-- Bootstrap -->
    <link rel="stylesheet" href="~/clientside/plugins/bootstrap/bootstrap.min.css">
    <!-- FontAwesome -->
    <link rel="stylesheet" href="~/clientside/plugins/fontawesome/css/all.min.css">
    <!-- Animation -->
    <link rel="stylesheet" href="~/clientside/plugins/animate-css/animate.css">
    <!-- slick Carousel -->
    <link rel="stylesheet" href="~/clientside/plugins/slick/slick.css">
    <link rel="stylesheet" href="~/clientside/plugins/slick/slick-theme.css">
    <!-- Colorbox -->
    <link rel="stylesheet" href="~/clientside/plugins/colorbox/colorbox.css">
    <!-- Template styles-->
    <link rel="stylesheet" href="~/clientside/css/style.css">

</head>
<body>
    <div class="body-inner">

        <partial name="~/Views/Shared/Partials/_Header.cshtml" />

        @RenderBody()

        <partial name="~/Views/Shared/Partials/_Footer.cshtml" />
        <!-- Javascript Files
        ================================================== -->
        <!-- initialize jQuery Library -->
        <script src="~/clientside/plugins/jQuery/jquery.min.js"></script>
        <!-- Bootstrap jQuery -->
        <script src="~/clientside/plugins/bootstrap/bootstrap.min.js" defer></script>
        <!-- Slick Carousel -->
        <script src="~/clientside/plugins/slick/slick.min.js"></script>
        <script src="~/clientside/plugins/slick/slick-animation.min.js"></script>
        <!-- Color box -->
        <script src="~/clientside/plugins/colorbox/jquery.colorbox.js"></script>
        <!-- shuffle -->
        <script src="~/clientside/plugins/shuffle/shuffle.min.js" defer></script>


        <!-- Google Map API Key-->
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCcABaamniA6OL5YvYSpB3pFMNrXwXnLwU" defer></script>
        <!-- Google Map Plugin-->
        <script src="~/clientside/plugins/google-map/map.js" defer></script>

        <!-- Template custom -->
        <script src="~/clientside/js/script.js"></script>

    </div><!-- Body inner end -->

    @RenderSection("FormScripts", false)
    @RenderSection("PiChart", false)
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    @RenderSection("SweetAlert", false)
</body>

</html>